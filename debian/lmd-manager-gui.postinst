#!/bin/sh
# postinst script for lmd-manager
#
# see: dh_installdeb(1)

set -e


case "$1" in
    configure)
	n4d-modules enable-plugin /etc/n4d/conf.d/LmdManager.json || true
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
